{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "paths": {
      "*": [
        "*",
        "src/*"
        ]
    },
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "./node_modules"
  ],
  "compileOnSave": false
}
